 <!-- show exercice-->
 <%  @FeedCourses.exercices.chrono.each do |q| %>
    <!-- completed by exercice-->
    <% if q.completed_by(current_user) %>
        <%= render partial: "home/shared/exercices/completed", locals: {q: q} %>
    <% else %>
        <%= render partial: "home/shared/exercices/unattempted", locals: {q: q} %>
    <% end %>
<% end %>
