
<% @title="Fil des léçons" %>

<%= render "home/shared/navbar" %>   
<main class="container-fluid">
  <div class=" p-5 rounded">
  <div class="mt-20">
    <%= @FeedMaterial.each do |material| %>
      <div class="font-semibold my-2"> <%= material.title %></div>
    <% end %>
  </div>
    

    <% @FeedCourse.each do |feed| %>
      <div class="mt-2">
        <h2 class="font-semibold">
          <%= feed.title %>
        </h2>

        <div class="border rounded-md p-2">
          <%= feed.content %>
        </div>
      </div>
    <% end %>  
  </div>
</main>